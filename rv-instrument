RV_BENCH_BIN=benchmarks/bin
RV_TEST_BIN=tests/bin
RV_INST_BIN=lib/rv-predict-inst.jar
RV_ENGINE_BIN=lib/rv-predict-engine.jar
RV_LOG_BIN=lib/rv-predict-log.jar

#instrument
echo "instrumenting "$1
time java -Xmx1336m -cp $RV_BENCH_BIN:$RV_TEST_BIN:$RV_INST_BIN:$RV_ENGINE_BIN rvpredict.instrumentation.Main $1 -nosa
