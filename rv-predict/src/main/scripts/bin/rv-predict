#!/usr/bin/env bash
source "$(dirname "$0")/../lib/setenv"
ulimit -s `ulimit -H -s`
if [ -z "$RV_OPTS" ];
  then export RV_OPTS="-Xms64m -Xmx1024m -Xss4m"
fi

if "$(dirname "$0")/../lib/checkJava"; then
  java $RV_OPTS -ea -cp "$(dirname "$0")/../lib/rv-predict.jar" com.runtimeverification.rvpredict.engine.main.Main "$@"
else
  exit 1
fi
