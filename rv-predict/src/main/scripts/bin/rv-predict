#!/usr/bin/env bash
ulimit -s `ulimit -H -s`

if test -n "$JENKINS_RV_ROOT"; then
    bindir=${RV_ROOT:-/usr/local}/bin
    libdir=${RV_ROOT:-/usr/local}/lib
else
    bindir=$JENKINS_RV_ROOT/bin
    libdir=$JENKINS_RV_ROOT
fi

${bindir}/checkJava || exit 1

java -ea -jar ${libdir}/rv-predict.jar "$@"
